<%- include('header'); -%>
  <div class="box" id="heading">
    <h1 style="color:purple"><%= listTitle %></h1>
  </div>
  <div class="box">
    <%for(let i=0;i<list.length;i++){%>
      <div class="item">
        <input type="checkbox">
        <p><%= list[i] %></p>
      </div>

    <%}%>
    <form class="" action="/" method="post">
      <input type="text" name="input" placeholder="New Items" autocomplete="off">
      <button type="submit" name="button" value=<%= listTitle %>>+</button>
    </form>
  </div>
<%- include('footer'); -%>
